{
  "expo": {
    "name": "Linkly",
    "slug": "linkly",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/linkly-icon.png",
    "splash": {
      "image": "./assets/linkly-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.askari.linkly",
      "versionCode": 1
    }
    
  }
}
